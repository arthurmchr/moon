{
  "name": "moon",
  "version": "1.0.0",
  "description": "Architecture",
  "author": "Arthur Muchir",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist app/index.html app/media/js/bundle.js app/media/js/bundle.js.map app/media/css/bundle.css app/media/css/bundle.css.map",
    "html": "node scripts/html.js",
    "css": "node scripts/postcss.js",
    "watch": "parallelshell 'npm run watch:html' 'npm run watch:css' 'npm run watch:lint' 'live-server --ignore=\"index.html.source,media/css/base,media/css/modules,media/css/layout,media/css/pages,media/css/utils,media/css/vendor,media/css/main.css,media/css/bundle.css.map\" app'",
    "watch:lint": "node scripts/eslint.js",
    "watch:html": "NODE_ENV=development npm run html",
    "watch:css": "NODE_ENV=development npm run css",
    "build": "npm run clean && npm run build:lint && npm run build:html && npm run build:css && npm run build:js",
    "build:lint": "eslint app/media/js/**/*.js",
    "build:html": "NODE_ENV=production npm run html",
    "build:css": "NODE_ENV=production npm run css",
    "build:js": "jspm bundle-sfx media/js/main.js app/media/js/bundle.js --minify",
    "deploy": "npm run build && rsync -r --exclude-from=rsyncignore app/ dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arthurmuchir/moon.git"
  },
  "bugs": {
    "url": "https://github.com/arthurmuchir/moon/issues"
  },
  "keywords": [
    "JSPM",
    "LiveReload",
    "PostCSS"
  ],
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "chokidar": "1.5.0",
    "cssnano": "3.5.2",
    "eslint": "2.9.0",
    "jspm": "0.16.34",
    "live-server": "1.0.0",
    "parallelshell": "2.0.0",
    "postcss": "5.0.21",
    "postcss-clearfix": "1.0.0",
    "postcss-custom-media": "5.0.1",
    "postcss-custom-properties": "5.0.1",
    "postcss-easy-import": "1.0.1",
    "postcss-fontpath": "0.3.0",
    "postcss-for": "2.0.2",
    "postcss-media-minmax": "2.1.2",
    "postcss-mixins": "4.0.1",
    "postcss-nested": "1.0.0",
    "postcss-position": "0.4.0",
    "postcss-reporter": "1.3.3",
    "postcss-simple-extend": "1.0.0",
    "postcss-single-charset": "1.0.0",
    "postcss-size": "1.0.0",
    "postcss-url": "5.1.2",
    "postcss-vertical-rhythm": "1.1.5",
    "preprocess": "3.1.0"
  },
  "jspm": {
    "directories": {
      "baseURL": "app"
    },
    "dependencies": {
      "babel-polyfill": "npm:babel-polyfill@6.7.4",
      "component-emitter": "npm:component-emitter@1.2.0",
      "gsap": "npm:gsap@1.18.2",
      "json": "github:systemjs/plugin-json@0.1.0",
      "page": "npm:page@1.7.1"
    },
    "devDependencies": {
      "babel": "npm:babel-core@5.8.24",
      "babel-runtime": "npm:babel-runtime@5.8.24",
      "core-js": "npm:core-js@1.1.4"
    }
  }
}
