{
  "name": "moon",
  "version": "1.0.0",
  "description": "Architecture",
  "author": "Arthur Muchir",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist app/index.html app/media/js/bundle.js app/media/js/bundle.js.map app/media/css/bundle.css app/media/css/bundle.css.map",
    "html": "node scripts/html.js",
    "css": "node scripts/postcss.js",
    "watch": "parallelshell 'npm run watch:lint' 'npm run watch:js' 'npm run watch:html' 'npm run watch:css' 'live-server --no-browser --ignorePattern=\"^(?!(media/css/bundle\\.css|media/js/bundle\\.js)$)(.*\\.(js|css|source|map)$)\" app'",
    "watch:lint": "esw -w app/media/js",
    "watch:js": "rollup -c -w --environment BUILD:development",
    "watch:html": "NODE_ENV=development npm run html",
    "watch:css": "NODE_ENV=development npm run css",
    "build": "npm run clean && npm run build:lint && npm run build:js && npm run build:html && npm run build:css",
    "build:lint": "esw app/media/js",
    "build:js": "rollup -c -m --environment BUILD:production",
    "build:html": "NODE_ENV=production npm run html",
    "build:css": "NODE_ENV=production npm run css",
    "deploy": "npm run build && rsync -r --exclude-from=rsyncignore app/ dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arthurmuchir/moon.git"
  },
  "bugs": {
    "url": "https://github.com/arthurmuchir/moon/issues"
  },
  "keywords": [
    "LiveReload",
    "PostCSS",
    "CSSnano",
    "Autoprefixer",
    "Babel",
    "Rollup",
    "ESLint"
  ],
  "devDependencies": {
    "autoprefixer": "6.3.7",
    "babel-preset-es2015-rollup": "1.1.1",
    "chokidar": "1.6.0",
    "cssnano": "3.7.3",
    "eslint": "3.1.0",
    "eslint-watch": "2.1.13",
    "live-server": "1.0.0",
    "parallelshell": "2.0.0",
    "postcss": "5.1.0",
    "postcss-clearfix": "1.0.0",
    "postcss-custom-media": "5.0.1",
    "postcss-custom-properties": "5.0.1",
    "postcss-easy-import": "1.0.1",
    "postcss-fontpath": "0.3.0",
    "postcss-for": "2.0.3",
    "postcss-media-minmax": "2.1.2",
    "postcss-mixins": "5.0.0",
    "postcss-nested": "1.0.0",
    "postcss-reporter": "1.4.1",
    "postcss-simple-extend": "1.0.0",
    "postcss-single-charset": "1.0.0",
    "postcss-url": "5.1.2",
    "postcss-vertical-rhythm": "1.1.5",
    "preprocess": "3.1.0",
    "rollup": "0.34.1",
    "rollup-plugin-babel": "2.6.1",
    "rollup-plugin-json": "2.0.1",
    "rollup-plugin-uglify": "1.0.1",
    "rollup-watch": "2.5.0"
  },
  "dependencies": {
    "component-emitter": "1.2.1",
    "page": "1.7.1"
  }
}
